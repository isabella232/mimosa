{
    "name": "facts",
    "targets": [
        "*"
    ],
    "inventory": {
        "nodes": [
            {
                "name": "xxxx.compute.amazonaws.com",
                "config": {
                    "transport": "ssh",
                    "ssh": {
                        "user": "ubuntu",
                        "private-key": {
                            "key-data": "-----BEGIN RSA PRIVATE KEY-----\r\n\r\n-----END RSA PRIVATE KEY-----"
                        },
                        "host-key-check": false
                    }
                }
            }
        ]
    }
}